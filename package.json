{
  "name": "tygr-server",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "start": "run-p --silent watch build:w",
    "build": "tsc",
    "build:w": "tsc --watch",
    "watch": "nodemon --quiet --watch bin --on-change-only --exec \"npm run --silent watch:task\"",
    "watch:task": "run-p --silent --print-label lint test:debug serve",
    "serve": "node --require dotenv/config --enable-source-maps bin",
    "test": "cross-env NODE_ENV=test mocha",
    "test:debug": "npm test -- --inspect --inspect-publish-uid=http --inspect-brk",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "npm run lint:fix",
    "prettier:check": "prettier -c .",
    "prettier:fix": "prettier --write .",
    "prettier": "npm run prettier:fix",
    "quality:check": "run-s --print-label --silent lint:check prettier:check",
    "quality:fix": "run-p --print-label --silent lint:fix prettier:fix",
    "quality": "npm run quality:fix"
  },
  "mocha": {
    "timeout": 5000,
    "exit": true,
    "require": [
      "dotenv/config"
    ],
    "spec": "bin/**/*.spec.js"
  },
  "dependencies": {
    "@tygr/auth-server": "0.0.6-beta",
    "bcrypt": "^5.0.0",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.1.3",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "sequelize": "^6.3.5",
    "sequelize-noupdate-attributes": "^1.0.0",
    "supertest": "^4.0.2",
    "web-push": "^3.4.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.5",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/luxon": "^1.25.0",
    "@types/mocha": "^5.2.7",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/sequelize": "^4.28.8",
    "@types/supertest": "^2.0.10",
    "@types/web-push": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^4.2.0",
    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-alloy": "^3.8.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "luxon": "^1.25.0",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "typescript": "^4.0.3"
  }
}
